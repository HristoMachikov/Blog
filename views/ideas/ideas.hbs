<div id="ideas">
    <div class="row">
        {{#each listings}}
            <div class="col-md-6">
                <listings>
                    <small class="author">
                        {{this.author}}
                    </small>
                    <p>{{this.content}}
                    </p>

                    <small class="email">
                        {{this.email}}
                    </small>
                    <small class="phone">
                        {{this.phone}}
                    </small>

                </listings>
            </div>
        {{/each}}
    </div>
</div>


<h2><a href="/ideasForm">Populate your listing here here!</a></h2>
